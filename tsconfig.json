{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "target": "ES6",
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./*"
      ],
      "@/domains/*": [
        "./src/domains/*"
      ],
      "@/tweaks": [
        "./src/domains/tweaks"
      ],
      "@/tweaks/*": [
        "./src/domains/tweaks/*"
      ],
      "@/game-configs": [
        "./src/domains/game-configs"
      ],
      "@/game-configs/*": [
        "./src/domains/game-configs/*"
      ],
      "@/dashboard": [
        "./src/domains/dashboard"
      ],
      "@/dashboard/*": [
        "./src/domains/dashboard/*"
      ],
      "@/auth": [
        "./src/domains/auth"
      ],
      "@/auth/*": [
        "./src/domains/auth/*"
      ],
      "@/pc-info": [
        "./src/domains/pc-info"
      ],
      "@/pc-info/*": [
        "./src/domains/pc-info/*"
      ],
      "@/shared": [
        "./src/domains/shared"
      ],
      "@/shared/*": [
        "./src/domains/shared/*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
